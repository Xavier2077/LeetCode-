class Solution {
public:
    ListNode* deleteDuplicates(ListNode* head) {
        if(!head)
        {
            return head;
        }
        ListNode* slow = head;
        ListNode* fast = head->next;
        while(fast)
        {
            if(slow->val == fast->val)
            {
                slow->next = fast->next;
                fast = slow->next;
                continue;
            }
            slow = fast;
            fast = fast->next;
        }
        return head;
    }
};
